{
  "name": "gatsby-rst-demo",
  "version": "1.0.0",
  "private": true,
  "description": "My Name",
  "author": "My Name",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby clean ; gatsby develop",
    "start": "gatsby clean ; gatsby develop",
    "build": "gatsby clean ; gatsby build",
    "serve": "gatsby clean ; gatsby serve",
    "format": "eslint --fix src/ && prettier --print-width 120 --write src/",
    "clean": "gatsby clean; npm run rebuild",
    "previews": "ls static/talks/*.pdf | xargs -I {} gs -dBATCH -dNOPAUSE -dQUIET -dFirstPage=1 -dLastPage=1 -sDEVICE=pngalpha -sOutputFile={}.png -r70 {}",
    "rebuild": "find content/ -name '*.txt.html' | xargs -r rm ; find content/ -name '*.txt.xml' | xargs -r rm",
    "deploy": "gatsby clean; npm run rebuild && npm run previews && gatsby build && rsync -a public/ <host>:<path> ; gatsby clean"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "@heroicons/react": "^2.0.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gatsby": "^5.2.0",
    "gatsby-plugin-catch-links": "^5.2.0",
    "gatsby-plugin-feed": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-layout": "^4.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-matomo": "^0.14.0",
    "gatsby-plugin-no-index": "^1.0.2",
    "gatsby-plugin-offline": "^6.2.0",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-plugin-react-svg": "^3.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-json": "^5.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "milligram": "^1.4.1",
    "overmind": "^28.0.2",
    "overmind-react": "^29.0.2",
    "prettier": "^2.8.0",
    "react": "^18.0",
    "react-dom": "^18.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.41.1",
    "xml2json": "^0.12.0"
  }
}
